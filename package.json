  "license": "MIT",
  "scripts": {
    "build": "gatsby build --log-pages",
    "build:with-storybook": "rm -rf public/storybook && yarn build && yarn storybook:build && mv storybook-static public/storybook",
    "clean": "gatsby clean",
    "develop": "GATSBY_DEBUG_MEDIA_QUERIES=true yarn gatsby develop -H 0.0.0.0",
    "generate": "node plugins/gatsby-ample-generator/lib/generate",
    "lint": "eslint -c .eslintrc.js ./*src && stylelint --config stylelint.config.js './*src/**/*.scss'",
    "netlify:dev": "netlify dev",
    "postinstall": "husky install",
    "serve": "gatsby serve -H 0.0.0.0",
    "storybook:build": "yarn test:generate-output && build-storybook --modern -c .storybook -s ./static",
    "storybook": "yarn test:generate-output && BROWSER=none start-storybook --modern -s ./static -p 6006 --quiet",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json --silent",
    "test:update": "yarn test --updateSnapshot",
    "test": "jest --silent"
  },
  "repository": {
    "type": "git",
