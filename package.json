{
  "name": "site",
  "private": true,
  "description": "Ample Gatsby Starter",
  "version": "0.1.0",
  "dependencies": {
    "docz": "latest",
    "gatsby": "^2.18.12",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-catch-links": "^2.1.15",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-theme-ample-components": "https://github.com/ample/gatsby-theme-ample-components",
    "gatsby-theme-docz": "latest",
    "gatsby-transformer-sharp": "^2.3.7",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "keywords": [
    "ample",
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop:clean": "gatsby clean && gatsby develop",
    "develop:with_host": "gatsby develop --open --host $HOSTNAME",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "serve": "gatsby clean && gatsby build && gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ample/gatsby-starter-ample"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
