{
  "name": "site",
  "private": true,
  "description": "Ample Gatsby Starter",
  "version": "0.1.0",
  "dependencies": {
    "@danbruegge/gatsby-plugin-stylelint": "^4.0.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-preset-gatsby": "^0.2.29",
    "classnames": "^2.2.6",
    "docz": "latest",
    "eslint": "^6.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "fs-extra": "^8.1.0",
    "gatsby": "^2.18.12",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-catch-links": "^2.1.15",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-postcss": "^2.1.20",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-sass": "^2.1.28",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-theme-ample-components": "https://github.com/ample/gatsby-theme-ample-components",
    "gatsby-theme-docz": "latest",
    "gatsby-transformer-sharp": "^2.3.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "pluralize": "^8.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^4.0.1",
    "postcss-responsive-type": "^1.0.0",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.18.2",
    "prettier-stylelint": "^0.4.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-test-renderer": "^16.12.0",
    "sass": "^1.25.0",
    "stylelint": "^13.2.0",
    "stylelint-formatter-pretty": "^1.1.4",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.14.2",
    "yargs": "^15.1.0"
  },
  "keywords": [
    "ample",
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop:clean": "gatsby clean && gatsby develop",
    "develop:with_host": "gatsby develop --open --host $HOSTNAME",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint -c .eslintrc.js .",
    "lint:js:fix": "eslint -c .eslintrc.js . --fix",
    "lint:styles": "stylelint --config stylelint.config.js 'src/**/*.scss'",
    "lint:styles:fix": "stylelint --config stylelint.config.js 'src/**/*.scss' --fix",
    "serve": "gatsby clean && gatsby build && gatsby serve",
    "clean": "gatsby clean",
    "test": "jest ./src",
    "test:update": "jest ./src --updateSnapshot"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ample/gatsby-starter-ample"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
