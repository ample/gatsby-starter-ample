  },
  "keywords": [
    "ample",
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build:with-storybook": "yarn build --log-pages && yarn storybook:build && mv storybook-static public/static/storybook",
    "clean": "gatsby clean",
    "develop": "GATSBY_DEBUG_MEDIA_QUERIES=true yarn gatsby develop -H 0.0.0.0",
    "forestry:preview": "gatsby develop -p 8080 -H 0.0.0.0",
    "generate": "node plugins/gatsby-ample-generator/lib/generate",
    "lint": "eslint -c .eslintrc.js ./src && stylelint --config stylelint.config.js './src/**/*.scss'",
    "netlify:dev": "netlify dev",
    "postinstall": "husky install",
    "serve": "gatsby serve",
    "storybook:build": "yarn run test:generate-output && build-storybook -c .storybook",
    "storybook": "yarn run test:generate-output && BROWSER=none start-storybook -s ./static -p 6006 --no-manager-cache",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json --silent",
    "test:update": "yarn test --updateSnapshot",
    "test": "jest --silent"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ample/gatsby-starter-ample"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
