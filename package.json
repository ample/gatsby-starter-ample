    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-test-renderer": "^16.12.0",
    "remark": "^12.0.0",
    "remark-html": "^11.0.2",
    "sass": "^1.26.0",
    "stylelint": "^13.2.0",
    "stylelint-formatter-pretty": "^1.1.4",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.14.2",
    "yargs": "^15.1.0"
  },
  "keywords": [
    "ample",
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn gatsby build",
    "clean": "yarn gatsby clean",
    "develop": "yarn gatsby develop",
    "develop:debug": "yarn run test && GATSBY_DEBUG_MEDIA_QUERIES=true yarn develop",
    "develop:with_host": "yarn develop --open --host $HOSTNAME",
    "forestry:preview": "gatsby develop -p 8080 -H 0.0.0.0",
    "generate": "node lib/generate.js",
    "lint": "eslint -c .eslintrc.js ./src && stylelint --config stylelint.config.js './src/**/*.scss'",
    "netlify:build": "yarn lint && yarn test && yarn clean && yarn build",
    "netlify:build:with_storybook": "yarn netlify:build && yarn storybook:build && mv storybook-static public/storybook",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook --quiet",
    "test": "jest --json --updateSnapshot --outputFile=.jest-test-results.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ample/gatsby-starter-ample"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
