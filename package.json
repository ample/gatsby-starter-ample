    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-essentials": "^5.3.18",
    "@storybook/addon-jest": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-gatsby": "^0.2.29",
    "core-js": "^3.0.1",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "fs-extra": "^8.1.0",
    "gatsby": "^2.20.14",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-catch-links": "^2.1.15",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-netlify": "^2.2.1",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-sass": "^2.1.28",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-plugin-sitemap": "^2.3.1",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-theme-ample-components": "ample/gatsby-theme-ample-components",
    "gatsby-transformer-sharp": "^2.3.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "pluralize": "^8.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^4.0.1",
    "postcss-responsive-type": "^1.0.0",
    "prettier": "^1.18.2",
    "prettier-stylelint": "^0.4.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-test-renderer": "^16.12.0",
    "sass": "^1.26.0",
    "stylelint": "^13.2.0",
    "stylelint-formatter-pretty": "^1.1.4",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.14.2",
    "yargs": "^15.1.0"
  },
  "keywords": [
    "ample",
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "develop:debug": "GATSBY_DEBUG_MEDIA_QUERIES=true yarn develop",
    "develop:with_host": "yarn develop --open --host $HOSTNAME",
    "lint": "eslint -c .eslintrc.js ./src && stylelint --config stylelint.config.js './src/**/*.scss'",
    "netlify:build": "yarn lint && yarn test && yarn clean && yarn build",
    "netlify:build:with_storybook": "yarn netlify:build && yarn storybook:build && mv storybook-static public/storybook",
    "storybook": "yarn test --json --outputFile=.jest-test-results.json && start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test:update": "yarn test --updateSnapshot",
    "test": "jest ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ample/gatsby-starter-ample"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
